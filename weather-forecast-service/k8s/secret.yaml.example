apiVersion: v1
kind: Secret
metadata:
  name: weather-forecast-secret
  labels:
    app: weather-forecast-service
type: Opaque
stringData:
  # Database credentials (replace with your actual values)
  db-username: "weather_user"
  db-password: "weather_pass"

  # Meteosource API Key (replace with your actual API key)
  meteosource-api-key: "your_meteosource_api_key_here"



